<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.harpreet.dicerollingapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/dicerollingappicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dicerollingappicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.DiceRollingApp">

        <activity android:name=".RollhistoryActivity"></activity>
        <activity
            android:name=".CustomDiceHistoryDialogActivity"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiceRollingActivity" />
    </application>

</manifest>